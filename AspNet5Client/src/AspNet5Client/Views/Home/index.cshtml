<h1>Home</h1>

@if (ViewBag.error != "")
{
    Console.WriteLine($"< div > {ViewBag.error} </ div >");
}
<div>
    <a href="home/callapi">Call API</a>
</div>

<div>
    <a href="home/signout">Sign out</a>
</div>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>